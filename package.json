{
  "name": "enterprise-wc",
  "version": "5.0.0",
  "description": "The Web Component version of the IDS Enterprise component library",
  "main": "app/index.js",
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "lint": "npx eslint *.js src test app && npx stylelint 'src/**/*.scss' --config .stylelintrc && npm run mdlint",
    "mdlint": "remark .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage-only-changed": "jest --coverage --onlyChanged",
    "test:ci": "jest --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "ncu": "npx ncu"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-markdown-style-guide"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infor-design/enterprise-wc.git"
  },
  "bugs": {
    "url": "https://github.com/infor-design/enterprise-wc/issues"
  },
  "author": "Hook & Loop",
  "license": "Apache-2.0",
  "homepage": "https://design.infor.com",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-wc": "^1.2.0",
    "favicons-webpack-plugin": "^3.0.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "ids-identity": "^3.0.0-beta.1",
    "install": "^0.13.0",
    "jest": "^26.1.0",
    "jest-puppeteer": "^4.4.0",
    "loglevel": "^1.6.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm": "^6.14.5",
    "pino": "^6.3.2",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^4.0.1",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-markdown-style-guide": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
